{
"coc.preferences.messageLevel": "warning",
"coc.preferences.hoverTarget": "float",
"coc.preferences.previewAutoClose": false,

"signature.target": "echo",
"signature.enable": true,

"suggest.maxCompleteItemCount": 20,
"suggest.floatEnable": true,
"suggest.enablePreview": true,
"suggest.echodocSupport": true,
"suggest.minTriggerInputLength": 2,
"suggest.keepCompleteopt": true,

"diagnostic.enable": false,
"diagnostic.enableSign": false,
"diagnostic.messageTarget": "float",

"jedi.enable": true,
"jedi.startupMessage": false,
"jedi.markupKindPreferred": "markdown",
"jedi.trace.server": "off",
"jedi.jediSettings.autoImportModules": ["numpy", "pandas"],
"jedi.executable.command": "jedi-language-server",
"jedi.executable.args": [],
"jedi.completion.disableSnippets": false,
"jedi.diagnostics.enable": true,
"jedi.diagnostics.didOpen": false,
"jedi.diagnostics.didChange": false,
"jedi.diagnostics.didSave": true,
"jedi.workspace.extraPaths": [],

"languageserver": {
  "lua": {
    "command": "/Users/wangk/.luarocks/bin/lua-lsp",
    "filetypes": ["lua"]
  }
},

"yank.highlight.enable": false,
"yank.list.maxsize": 50,
"yank.enableCompletion": false,

"list.insertMappings": {
    "<C-r>": "do:refresh",
    "<C-n>": "normal:j",
    "<C-p>": "normal:k",
    "<C-g>": "do:exit"
    },

"suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
    },

"explorer.icon.enableNerdfont": true,
"explorer.previewAction.onHover": false,
"explorer.icon.enableVimDevicons": true,
"explorer.file.autoReveal": false,
"explorer.floating.border.chars": ["─", "│", "─", "│", "╭", "╮", "╯", "╰"],
"explorer.quitOnOpen": true,
"explorer.file.root.template": "[icon] [title] [hidden & 1] [fullpath]",
"explorer.file.child.template": "[selection | clip | 1] [indent][icon | 1] [filename omitCenter 1] [link growRight 1 omitCenter 5] [size]",
"explorer.sources": [
    {"name": "buffer","expand": true},
    {"name": "file","expand": true}
    ],

"explorer.keyMappings.global": {
    "i": false,

    "+": "toggleSelection",
    "<tab>": "actionMenu",

    "h": "collapse",
    "l": ["expandable?", "expand", "open"],
    "o": ["expanded?", "collapse", "expand"],
    "<cr>": ["expandable?", "cd", "open"],
    "<C-x>": "open:split",
    "<C-v>": "open:vsplit",
    "<C-t>": "open:tab",
    "<bs>": "gotoParent",
    "gp": "preview:labeling",

    "y": "copyFilepath",
    "Y": "copyFilename",
    "c": "copyFile",
    "x": "cutFile",
    "p": "pasteFile",
    "d": "delete",

    "N": "addFile",
    "r": "rename",

    ".": "toggleHidden",
    "R": "refresh",

    "?": "help",
    "q": "quit",
    "<C-g>": "quit",
    "<esc>": "esc",
    "X": "systemExecute",

    "f": "searchRecursive",

    "[[": "sourcePrev",
    "]]": "sourceNext",

    "[c": "gitPrev",
    "]c": "gitNext",
    ">>": "gitStage",
    "<<": "gitUnstage"
    },
    "clangd.path": "/Users/wangk/.config/coc/extensions/coc-clangd-data/install/11.0.0/clangd_11.0.0/bin/clangd"
}

