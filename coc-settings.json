{
"coc.preferences.messageLevel": "warning",
"coc.preferences.hoverTarget": "float",
"coc.preferences.previewAutoClose": false,

"signature.target": "echo",
"signature.enable": true,

"suggest.maxCompleteItemCount": 20,
"suggest.floatEnable": true,
"suggest.enablePreview": true,
"suggest.echodocSupport": true,
"suggest.minTriggerInputLength": 2,
"suggest.keepCompleteopt": false,

"diagnostic.enable": false,

"python.autoComplete.extraPaths": ["/Users/wangk/anaconda3/lib/python3.7/site-packages/"],
"python.terminal.executeInFileDir": true,
"python.pythonPath": "/Users/wangk/anaconda3/bin/python3",
"python.condaPath": "/Users/wangk/anaconda3/bin/conda",
"python.formatting.provider": "yapf",
"python.analysis.diagnosticEnabled": false,
"python.linting.enabled": false,

"yank.highlight.enable": false,
"yank.list.maxsize": 50,
"yank.enableCompletion": false,

"languageserver": {
    "clangd": {
      "command": "ccls",
      "rootPatterns": ["compile_flags.txt", "compile_commands.json", ".git/"],
      "filetypes": ["c", "cc", "cpp", "hpp", "c++", "objc", "objcpp"]
    },
    "lua": {
      "command": "lua-lsp",
      "filetypes": ["lua"]
    }
  },

